--<+>/* ATB Blue = #69a695 */\n/* Grey Buttons/Hovers = #d9d9d9 */\n/* Super light grey, almost white background paneling = #f6f6f6 */\n\n/* Font Styling for entire website */\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@600&display=swap');\n\nhtml body p {\n    font-family: \"Open Sans\";\n}\n\nh1 {\n    font-: 100px;\n}\n/*navbar*/\nnavbar {\n    display: flex;\n    justify-content: space-between;\n    padding: 20px;\n    background-color: #69a695;\n    color: #fff;\n}\n.navbar-nav a {\n    float: right;\n}\n\n/* allows for hovering over the navbar*/\n.navbar-nav li a:hover {\n     background: #d9d9d9;\n }\n\nlabel {\n    font-size: 20px;\n}\n\nbackground-image {\n    opacity: 0.5;\n}\n\nurl(\"static/images/WelcomePage_BackgroundPhoto.jpeg\") {\nopacity: 0.5;\n}\n\n/*use this to create a division that has centered text*/\n.login-buttons{\n    text-align: center;\n}\n\n\n.text-center{\n    text-align: center;\n\n}\n\n/* button styling */\ninput{\n    background-color: #69a695;\n    text-color: white;\n    border-width: 1px;\n    border-color: black;\n    border-radius: 5px;\n    padding: 7px;\n    margin: 2px;\n    color: white;\n    text-color: white;\n}\n\n.checkoutButtons{\n    background-color: #69a695;\n    font-family: 'Open Sans';\n    color: white;\n}\n\np {\n    color: black;\n    font-family: 'Open Sauce SemiBold';\n    font-size: 20px;\n\n}\n\n/* Checkout Page Styling */\n\npostFormLabels {\n    display: inline-block;\n    text-align: left;\n}\n\n.checkoutPlaceholder {\n    height: 210px;\n    width: 1200px;\n    background-color: #f6f6f6;\n    padding: 0px 0px 0px 0px;\n}\n\ndiv.checkoutItemCoupon{\n    display: block;\n    padding: 20px;\n}\n\ndiv.coupon{\n    display: inline-block;\n}\n\n\n.formInputFields {\n    background-color: #f6f6f6;\n    padding: 30px 25px 30px 25px;\n}\n\n.formInputFieldsNoBackground {\n    padding-left: 20px;\n}\n\n.checkoutHeader {\n    font-size: 33px;\n    float: left;\n}\n\n.sectionBreaker {\n    