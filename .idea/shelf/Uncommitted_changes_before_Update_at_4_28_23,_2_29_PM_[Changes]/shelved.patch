in-left: 10px;\n    width: 40px;\n    height: 40 px;\n    text-align: center;\n    line-height:40px;\n    cursor: pointer;\n}\n\n.page-btn span:hover {\n    background: #69a695;\n    color: white\n}\n\n*{\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n}\n\nh3.shopLabels {\n    padding: 0px 0px 0px 25px;\n\n}\n\np.shopLabels {\n    padding: 0px 0px 0px 25px;\n\n}\n\n.containera{\n    max-width: 1300px;\n    margin: auto;\n    padding-left:25px;\n    padding-right:25px;\n}\n\n.col-5{\n    flex-basis: 20%;\n    min-width:300px;\n}\n\n.img fit {\n    max-width:100%;\n    padding:50px 0;\n}\n\n.image-container {\n  display: flex;\n  flex-wrap: wrap;\n  flex-direction: row;\n  align-items: center;\n  justify-content: center;\n  position: absolute;\n  top: 450px;\n  left: 100px;\n}\n\n.image-container img {\n  width: 200px;\n  height: 200px;\n  object-fit: cover;\n}\n\n.ContactUs_MainHeading {\n    text-align: center;\n     color: blue;\n}\n\n/*creates two row layout for entire page*/\n.GenProduct_rowLayout {\n  display: flex;\n  margin-left: 10px;\n}\n\n/*creates two row layout for entire page*/\n.GenProduct_rowLayout {\n  display: flex;\n}\n\n/*creates spacing between each element on the right side of the generic product page*/\n.GenProduct_spacer{\n  margin-top: 50px;\n}\n\n/* creates styling for quantity button on Generic Product Page*/\ninput.btneg {\n  border-radius: 5cm;\n  width: 40px;\n  position: relative;\n  margin-right: -30px;\n  height: 0.8cm;\n}\n\n/* creates styling for quantity button on Generic Product Page*/\ninput.btpos {\n  border-radius: 5cm;\n  width: 40px;\n  position: relative;\n  margin-left: -30px;\n  height: 0.8cm;\n}\n\n/*creates spacing between both columsn of information on the generic product page*/\n.GenProduct_VerticalSpacer{\n  margin-left: 30px;\n}\n\n.GenProduct_HeadingSpacer{\n    margin-bottom: 20px;\n}\n\n/* creates styling for Mission Statement on About Page*/\n\n.g {\n  background-color: lightgrey;\n  max-width:100%;\n  padding: 50px;\n  margin: 20px;\n}\n\ndiv.homeTitle{\n    display: block;\n    padding-left: 30px;\n    font-size: large;\n}\n\n.slogan {\n    float: left;\n    width: 40%;\n    font-size: 150px;\n    font-family: 'Open Sans';\n    position:absolute;\n    left: 100px;\n    top: 200px;\n}\n\n.columnRight {\n    float: right;\n    display: flex;\n}\n\n.map {\n    width: 50%;\n    height: auto;\n    border: 10px solid #69a695;\n}\n\n/* About Us Page Containers*/\n\n.containerd {\n  max-width: 100%;\n  margin: 0 auto;\n  padding: 70px;\n  background-color: #f1f1f1;\n  border: 1px solid #ccc;\n}\n\n.containere {\n  max-width: 100%; /* Set the maximum width of the container */\n  margin: 0 auto; /* Center the container horizontally */\n  padding: 70px; /* Add some padding to the container */\n  background-color: #DCDCDC; /* Set the background color of the container */\n  border: 1px solid #ccc; /* Add a border around the container */\n}\n\n\n.leftcolumn{\n    float: left;\n    width: 60%;\n}\n.rightcolumn{\n    float: left;\n    width: 40%;\n    padding-left: 20px;\n}\n
