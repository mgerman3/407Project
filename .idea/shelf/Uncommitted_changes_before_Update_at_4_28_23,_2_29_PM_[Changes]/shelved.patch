nner():\n    return render_template('Banner.html')\n\n@app.route('/cart')\ndef Cart():\n    return render_template('cart.html')\n\n@app.route('/InventoryLog')\ndef InventoryLog():\n    return render_template('Inventory Log.html')\n\nif __name__ == '__main__':\n    app.run()\n
