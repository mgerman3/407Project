<+>/* ATB Blue = #69a695 */\n/* Grey Buttons/Hovers = #d9d9d9 */\n/* Super light grey, almost white background paneling = #f6f6f6 */\n\n/* Font Styling for entire website */\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@600&display=swap');\n\nhtml body p {\n    font-family: \"Open Sans\";\n}\n\nh1 {\n    font-: 100px;\n}\n/*navbar*/\nnavbar {\n    display: flex;\n    justify-content: space-between;\n    padding: 20px;\n    background-color: #69a695;\n    color: #fff;\n}\n.navbar-nav a {\n    float: right;\n}\n\n/* allows for hovering over the navbar*/\n.navbar-nav li a:hover {\n     background: #d9d9d9;\n }\n\nlabel {\n    font-size: 20px;\n}\n\nbackground-image {\n    opacity: 0.5;\n}\n\nurl(\"static/images/WelcomePage_BackgroundPhoto.jpeg\") {\nopacity: 0.5;\n}\n\n/*use this to create a division that has centered text*/\n.login-buttons{\n    text-align: center;\n}\n\n\n.text-center{\n    text-align: center;\n\n}\n\n/*use this to style create role dropdown on admin new login page*/\nselect#role.form-select{\n    width: 800px;\n    height: 40px;\n    border-radius: 5px;\n    border-color: black;\n    background-color: #f6f6f6;\n    color: black;\n}\n\n\n/* button styling */\n.input{\n    background-color: #69a695;\n    text-color: white;\n    border-width: 1px;\n    border-color: black;\n    border-radius: 5px;\n    padding: 7px;\n    margin: 2px;\n    color: white;\n    text-color: white;\n}\n\ninput.checkoutButtons{\n    background-color: #69a695;\n    font-family: 'Open Sans';\n    color: white;\n    width: 90%;\n}\n\np {\n    color: black;\n    font-family: 'Open Sauce SemiBold';\n    font-size: 20px;\n\n}\n\n.websiteFooter {\n    background-color: #69a695;\n    margin-top: 7px;\n    padding-top: 7px;\n    padding-bottom: 7px;\n    display: block;\n}\n\na.footerItems{\n    color: black;\n    text-align: left;\n    padding-left: 50px;\n    text-decoration: none;\n}\n\np.footerItems{\n    padding-left: 12px;\n    text-decoration: underline;