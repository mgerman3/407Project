nputFields {\n    height: 30px;\n    background-color: #f6f6f6;\n    border-color: black;\n    width: auto;\n    color: black;\n}\n\n.formFieldsLabels {\n    font-size: 20px;\n    margin: 0px;\n    padding: 0px;\n    text-align: left;\n}\n\np.cartTotalsLabels {\n    font-size: 25px;\n    margin: 0px;\n    padding: 0px 0px 20px 0px;\n    text-align: left;\n}\n\nhr {\n    margin: 0px;\n}\n\n/*Styling order confirmation page*/\n.h1-Center {\n    text-align: center;\n}\n\nh2.left {\n    text-align:left;\n}\n\n/* needed for inputing inventory pages while using jinja */\nh2 {\n    text-align: center;\n}\n\np {\n    text-align: left;\n}\n\ntext.left {\n    text-align:left;\n}\n\n.center {\n  margin: auto;\n  width: 50%;\n  border: 3px solid black;\n  padding: 10px;\n}\n/*center image*/\n.center-image {\n            display:block;\n            margin-left: auto;\n            margin-right: auto;\n        }\n\na.btn.btn-secondary {\n    background-color: #69a695;\n    color: white;\n    font-size: 17px;\n    border: 1px solid black;\n    border-radius: 5px;\n    padding: 5px 5px 5px 5px;\n    text-decoration: none;\n}\n\n\n/*Filter shop button styling*/\n/* Dropdown Button */\n.dropbtn {\n  background-color: #04AA6D;\n  color: black;\n  padding: 16px;\n  font-size: 16px;\n  border: none;\n}\n\n/* The container <div> - needed to position the dropdown content */\n.dropdown {\n  position: relative;\n  display: inline-block;\n    font-size: 20px;\n}\n\n/* Dropdown Content (Hidden by Default) */\n.dropdown-menu {\n  display: none;\n  position: absolute;\n  background-color: #69a695;\n  min-width: 160px;\n  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\n  z-index: 1;\n}\n\n/* Links inside the dropdown */\n.dropdown-content a {\n  color: black;\n  padding: 12px 16px;\n  text-decoration: none;\n  display: block;\n}\n\n\n/* allows for hovering over navbar to show the dropdown menu */\n.dropdown:hover .dropdown-menu {display: block;\n    position: absolute;\n    background-color: #69a6