<+><!-- Extending base code to use Jinja principles -->\n{% extends \"base.html\" %}\n\n{% block title %}Check Out{% endblock %}\n\n{% block page_content %}\n\n    {% if cart_count > 0 %}\n        <div class=\"row\">\n            <div class=\"col-lg-3\">\n\n            </div>\n            <div class=\"col-lg-2\">\n                <h3 class=\"bold\">Item</h3>\n            </div>\n            <div class=\"col-lg-2\">\n                <h3>Quantity</h3>\n            </div>\n            <div class=\"col-lg-2\">\n                <h3>Price</h3>\n            </div>\n            <div class=\"col-lg-1\">\n                <h3>Size</h3>\n            </div>\n            <div class=\"col-lg-2\">\n                <h3>Total</h3>\n            </div>\n        </div>\n\n        {% set ns = namespace(item = 0) %}\n\n        {% for product in products %}\n            {% if product['product_quantity'] > 0 %}\n                <div class=\"row mb-3\">\n                    <div class=\"col-lg-3\">\n                        <div class=\"col-xs-12 col-sm-12 col-md-12 col-lg-12\">\n                            <div class=\"thumbnail thumbnail-q\">\n                                <img class=\"img-fluid\" src=\"{{ url_for('static', filename='/products/' + product['product_image']) }}\" alt=\"Picture of {{ product['item_name'] }}\">\n                            </div>\n                        </div>\n                    </div>\n                    <div class=\"col-lg-2\">\n                        <strong>{{ product['item_name'] }}</strong>\n                        <br>\n                        <a href=\"{{ url_for('cart_remove', index=ns.item) }}\" class=\"btn btn-danger btn-sm\">Remove</a>\n                    </div>\n                    <div class=\"col-lg-2\">\n                        <strong>{{ product['product_quantity'] }}</strong>\n                    </div>\n                    <div class=\"col-lg-2\">\n                        <strong>{{ \"${:,.2f}\".format(product['price']) }}</strong>\n 