----+<+>/* ATB Blue = #69a695 */\n/* Grey Buttons/Hovers = #d9d9d9 */\n/* Super light grey, almost white background paneling = #f6f6f6 */\n\n/* Font Styling for entire website */\n@import url('https://fonts.googleapis.com/css2?family=Open+Sans:wght@600&display=swap');\n/*navbar*/\nnavbar {\n    display: flex;\n    justify-content: space-between;\n    padding: 20px;\n    background-color: #69a695;\n    color: #fff;\n}\n.navbar-nav a {\n    float: right;\n}\n\nbackground-image {\n    opacity: 0.5;\n}\n\nurl(\"static/images/WelcomePage_BackgroundPhoto.jpeg\") {\nopacity: 0.5;\n}\n\nbody{\n    font-family: 'Open Sauce SemiBold';\n}\n\nh1 {\n    color: black;\n    font-family: 'Open Sauce SemiBold';\n    font-size: 80px;\n}\n\n/*use this to create a division that has centered text*/\n.login-buttons{\n    text-align: center;\n}\n\n\n.text-center{\n    text-align: center;\n\n}\n\n/* button styling */\ninput{\n    background-color: #69a695;\n<<<<<<< HEAD\n=======\n    text-color: white;\n>>>>>>> a0cf96c (This push includes numerous changes:)\n    border-width: 1px;\n    border-color: #545454;\n    border-radius: 5px;\n    padding: 7px;\n    margin: 2px;\n    color: white;\n}\n\n.checkoutButtons{\n    background-color: #69a695;\n    font-family: 'Open Sans';\n    color: white;\n}\n\np {\n    color: black;\n    font-family: 'Open Sauce SemiBold';\n    font-size: 20px;\n\n}\n\n/* Checkout Page Styling */\n\n.checkoutPlaceholder {\n    height: 210px;\n    width: 1200px;\n    background-color: #f6f6f6;\n}\n\ndiv.checkoutItemCoupon{\n    display: block;\n    padding: 20px;\n}\n\ndiv.coupon{\n    display: inline-block;\n}\n\n.formInputFields {\n    background-color: #f6f6f6;\n    padding: 30px 25px 30px 25px;\n}\nh2.checkoutHeader {\n    font-size: 33px;\n    float: left;\n}\n\n<<<<<<< HEAD\n=======\n.navbar {\n    background-color: deepskyblue;\n}\n\n>>>>>>> a0cf96c (This push includes numerous changes:)\n.inputBoxTotal {\n    padding: 20px;\n}\n\n.billingTotals {\n    padding: 30px 25px 30px 25px;\n}\n\n.inputFields {\n    height: 30px;\n    background-color: #f6f6f6;\n    width: auto;\n}\n\n<<<<<<< HEAD\np.formFieldLabels {\n=======\n.formFieldLabels {\n>>>>>>> a0cf96c (This push includes numerous changes:)\n    font-size: 20px;\n    margin: 0px 0px 0px 0px;\n    padding: 0px 0px 0px 0px;\n}\n\np.cartTotalsLabels {\n    font-size: 25px;\n    margin: 0px;\n    padding: 0px;\n}\n\n/*Styling order confirmation page*/\n/*Centering text*/\n.center {\n  margin: auto;\n  width: 50%;\n  border: 3px solid black;\n  padding: 10px;\n}\n/*center image*/\n.center-image {\n            display:block;\n            margin-left: auto;\n            margin-right: auto;\n\n        }\n\n/*Filter shop button styling*/\n/* Dropdown Button */\n.dropbtn {\n  background-color: #04AA6D;\n  color: white;\n  padding: 16px;\n  font-size: 16px;\n  border: none;\n}\n\n/* The container <div> - needed to position the dropdown content */\n.dropdown {\n  position: relative;\n  display: inline-block;\n}\n\n/* Dropdown Content (Hidden by Default) */\n.dropdown-content {\n  display: none;\n  position: absolute;\n  background-color: #d9d9d9;\n  min-width: 160px;\n  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\n  z-index: 1;\n}\n\n/* Links inside the dropdown */\n.dropdown-content a {\n  color: black;\n  padding: 12px 16px;\n  text-decoration: none;\n  display: block;\n}\n\n/* Change color of dropdown links on hover */\n.dropdown-content a:hover {background-color: #ddd;}\n\n/* Show the dropdown menu on hover */\n.dropdown:hover .dropdown-content {display: block;}\n\n/* Change the background color of the dropdown button when the dropdown content is shown */\n.dropdown:hover .dropbtn {background-color: #3e8e41;}\n\ntitle {\n    font-size: 30px;\n}\n
