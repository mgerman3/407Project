ent a {\n  color: black;\n  padding: 12px 16px;\n  text-decoration: none;\n  display: block;\n}\n\n\n/* allows for hovering over navbar to show the dropdown menu */\n.dropdown:hover .dropdown-menu {display: block;\n    position: absolute;\n    background-color: #69a695;\n    box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\n    z-index: 1;\n    margin: 50px 0px 0px 0px;\n    padding: 0px;\n}\n\n.dropdown-content {\n  display: none;\n  position: absolute;\n  background-color: #69a695;\n  min-width: 160px;\n  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);\n  z-index: 1;\n    font-size: 20px;\n}\n\n\n\ntitle {\n    font-size: 30px;\n}\n/* Styling for inventory management page */\n  .max-width-form {\n    max-width: 800px;\n    margin: 0 auto;\n      padding-top: 40px; ;\n  }\n\n  /* styling for home page */\n.bg-image {\n    position: fixed;\n    top: 0;\n    left: 0;\n    width: 100%;\n    height: 100%;\n    z-index: -1;\n    opacity: 0.5;\n}\n.container {\n    font-weight: bold;\n    font-size: 80px;\n    font-family: 'Open Sans';\n    padding-top: 30px;\n    position:relative;\n    left: 40px;\n}\n.slogan {\n    float: left;\n    width: 40%;\n    font-size: 150px;\n    font-family: 'Open Sans';\n    padding-top: 75px;\n    position:relative;\n    left: 100px;\n}\n\nform input[type=\"submit\"] {\n  background-color: #69a695;\n  color: white;\n  border-radius: 0px;\n    padding: 10px 15px 10px 15px;\n  font-size: 24px;\n}\n\ndropdown button {\n  background-color: #69a695;\n  color: white;\n  border-radius: 0px;\n    padding: 10px 15px 10px 15px;\n  font-size: 24px;\n}\n\nbr {\n   display: block;\n   margin: 4px;\n}\n\n/* styling for login input boxes */\nform input[class=\"loginInput\"] {\n  background-color: white;\n  color: black;\n  border-radius: 0px;\n  font-size: 40px;\n    width: 800px;\n    height: 100px;\n    margin: auto;\n  display: block;\n}\n\n.map {\n   position: absolute;\n    width: 400px;\n    top: 100px;\n    right: 300px;\n    border: 10px solid #69a695