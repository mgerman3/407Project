\n}\n\n.reviewHeader {\n    font-size: 33px;\n    padding-left: 20px;\n}\n\n\n<<<<<<< HEAD\n=======\n.navbar {\n    background-color: deepskyblue;\n}\n\n>>>>>>> a0cf96c (This push includes numerous changes:)\n.inputBoxTotal {\n    padding: 20px;\n}\n\n.billingTotals {\n    padding: 30px 25px 30px 25px;\n}\n\n.inputFields {\n    height: 30px;\n    background-color: #f6f6f6;\n    border-color: gray;\n    width: auto;\n}\n\n.formFieldsLabels {\n    font-size: 20px;\n    margin: 0px;\n    padding: 0px;\n}\n\np.cartTotalsLabels {\n    font-size: 25px;\n    margin: 0px;\n    padding: 0px 0px 20px 0px;\n}\n\nhr {\n    margin: 0px;\n}\n\n/*Styling order confirmation page*/\n/*Centering text*/\n.center {\n  margin: auto;\n  width: 50%;\n  border: 3px solid black;\n  padding: 10px;\n}\n/*center image*/\n.center-image {\n            display:block;\n            margin-left: auto;\n            margin-right: auto;\n        }\n\ntitle {\n    font-size: 30px;\n}\n\n.paddingbetweenelements {\n     padding-bottom: 20px;\n}\n\n/*Used to create and style rectangle images on Contact us page for each individual review*/\nrectangle {\n  height: 170px;\n  width: 350px;\n  background-color: blue;\n  /*outline-color: blue;*/\n}\n\n
