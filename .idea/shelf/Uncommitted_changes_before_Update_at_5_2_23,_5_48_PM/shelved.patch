---<+><!-- Joe Scopelliti -->\r\n\r\n<!DOCTYPE html>\r\n<html lang=\"en\">\r\n    <head>\r\n        <meta charset=\"UTF-8\">\r\n        <title>Check Out</title>\r\n        <link href=\"/static/CSS/style.css\" rel=\"stylesheet\">\r\n        <link href=\"https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css\" rel=\"stylesheet\" integrity=\"sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD\" crossorigin=\"anonymous\">\r\n    </head>\r\n    <body>\r\n        <!-- Navbar -->\r\n        <nav class=\"navbar navbar-expand-lg\" style=\"background-color: #69a695\">\r\n          <div class=\"container-fluid\">\r\n            <a class=\"navbar-brand\" href=\"#\"><img src=\"../static/images/Clear%20Background%20Logo.png\" alt=\"Bootstrap\" width=\"30\" height=\"24\"></a>\r\n            <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNavDropdown\" aria-controls=\"navbarNavDropdown\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n              <span class=\"navbar-toggler-icon\"></span>\r\n            </button>\r\n            <div class=\"collapse navbar-collapse\" id=\"navbarNavDropdown\">\r\n              <ul class=\"navbar-nav\">\r\n                <li class=\"nav-item\">\r\n                  <a class=\"nav-link active\" aria-current=\"page\" href=\"Home%20Page.html\">Home</a>\r\n                </li>\r\n                <li class=\"nav-item dropdown\">\r\n                  <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\r\n                    Shop\r\n                  </a>\r\n                  <ul class=\"dropdown-menu\">\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Game Day Collection</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Dark Blue Collection</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Springtime Collection</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Logo Collection</a></li>\r\n                  </ul>\r\n                </li>\r\n                <li class=\"nav-item dropdown\">\r\n                  <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\r\n                    About\r\n                  </a>\r\n                  <ul class=\"dropdown-menu\">\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Around the Block</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Our Mission</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Meet our Team</a></li>\r\n                  </ul>\r\n                </li>\r\n                <li class=\"nav-item dropdown\">\r\n                  <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\r\n                    Contact Us\r\n                  </a>\r\n                  <ul class=\"dropdown-menu\">\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Request Form</a></li>\r\n                    <li><a class=\"dropdown-item\" href=\"#\">Reviews</a></li>\r\n                  </ul>\r\n                </li>\r\n              </ul>\r\n            </div>\r\n          </div>\r\n        </nav>\r\n\r\n        {% if form_submitted %}\r\n        <div class=\"row\">\r\n            <div class=\"checkoutHeader\" style=\"padding: 20px\">\r\n                <h2>Thank you for your order!</h2><br>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>Products Ordered:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get(products)}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>First Name:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('first_name')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>Last Name:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('last_name')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n           <div class=\"postFormLabels\">\r\n                <p>Address 1:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('address1')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>Address 2:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('address2')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>City:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('city')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>State:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('state')}}</p>\r\n            </div>\r\n        </div>\r\n        <div class=\"row\">\r\n            <div class=\"postFormLabels\">\r\n                <p>Zip code:</p>\r\n            </div>\r\n            <div class=\"postFormLabels\">\r\n                <p>{{ request.form.get('zipcode')}}</p>\r\n            </div>\r\n        </div>\r\n        {% else %}\r\n        <!-- Coupon Code Box & Cart Information -->\r\n        <form action=\"{{ url_for('CheckOut') }}\" method=\"post\">\r\n