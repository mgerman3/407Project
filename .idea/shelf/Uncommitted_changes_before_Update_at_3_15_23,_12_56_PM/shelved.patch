ation\" novalidate>\n      <div class=\"col-md-4\">\n        <label for=\"validationItemID\" class=\"form-label\">Item ID</label>\n       <input type=\"text\" class=\"form-control\" id=\"validationItemID\" required>\n        <div class=\"valid-feedback\">\n        Looks good!\n    </div>\n  </div>\n  <div class=\"col-md-4\">\n    <label for=\"validationCollectionID\" class=\"form-label\">Collection ID</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationCollectionID\" required>\n    <div class=\"valid-feedback\">\n      Looks good!\n    </div>\n  </div>\n  <div class=\"col-md-4\">\n    <label for=\"validationItemName\" class=\"form-label\">Item Name</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationItemName\" required>\n    <div class=\"invalid-feedback\">\n      Please enter a valid Item Name.\n    </div>\n  </div>\n  <div class=\"col-md-3\">\n    <label for=\"validationItemSize\" class=\"form-label\">Item Size</label>\n    <select class=\"form-select\" id=\"validationItemSize\" required>\n      <option selected disabled value=\"\">Choose...</option>\n      <option>XS</option>\n      <option>S</option>\n      <option>M</option>\n      <option>L</option>\n      <option>XL</option>\n      <option>XXL</option>\n    </select>\n    <div class=\"invalid-feedback\">\n      Please select a valid size.\n    </div>\n  </div>\n  <div class=\"col-md-3\">\n    <label for=\"validationItemColor\" class=\"form-label\">Item Color</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationItemColor\" required>\n    <div class=\"invalid-feedback\">\n      Please provide a valid color.\n    </div>\n  </div>\n  <div class=\"col-md-3\">\n    <label for=\"validationPrice\" class=\"form-label\">Item Price</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationPrice\" required>\n    <div class=\"invalid-feedback\">\n      Please provide a valid price.\n    </div>\n  </div>\n  <div class=\"col-md-3\">\n    <label for=\"validationQuantityAvail\" class=\"form-label\">Quantity Available</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationQuantityAvail\" required>\n    <div class=\"invalid-feedback\">\n      Please provide a valid quantity.\n    </div>\n  </div>\n  <div class=\"col-md-4\">\n    <label for=\"validationImage\" class=\"form-label\">Insert Image</label>\n    <input type=\"file\" class=\"form-control\" id=\"validationImage\" required>\n    <div class=\"invalid-feedback\">\n      Please provide a valid image file.\n    </div>\n  </div>\n  <div class=\"col-md-8\">\n    <label for=\"validationDescription\" class=\"form-label\">Item Description</label>\n    <input type=\"text\" class=\"form-control\" id=\"validationDescription\" required>\n    <div class=\"invalid-feedback\">\n      Please provide a valid description.\n    </div>\n  </div>\n  <div class=\"col-12\">\n    <button class=\"btn btn-primary\" type=\"submit\">Submit form</button>\n  </div>\n  </div>\n</div>\n</\n<!-- Inventory Management Search Bar -->\n<div class=\"container\">\n  <div class=\"row justify-content-center\">\n    <div class=\"col-md-6\">\n      <form class=\"form-inline my-2 my-lg-0\">\n        <input class=\"form-control mr-sm-2\" type=\"search\" placeholder=\"Search Inventory...\" aria-label=\"Search\">\n        <button class=\"btn btn-outline-success my-2 my-sm-0\" type=\"submit\">Search</button>\n      </form>\n    </div>\n  </div>\n<!-- Inventory Management Table -->\n<div class=\"container\">\n  <table class=\"table table-striped\">\n    <thead>\n      <tr>\n          <th scope=\"col\">Item ID</th>\n          <th scope=\"col\">Collection ID</th>\n        <th scope=\"col\">Item Name</th>\n        <th scope=\"col\">Item Size</th>\n          <th scope=\"col\">Item Color</th>\n          <th scope=\"col\">Item Price</th>\n          <th scope=\"col\">Item Description</th>\n          <th scope=\"col\">Item Image</th>\n          <th scope=\"col\">Quantity Available</th>\n      </tr>\n